{
    "presets": ["react", "es2015"],
    "plugins": [
        [
            "module-alias", [{
                "src": "../../themes/",
                "expose": "themes"
            }]
        ],
        "add-module-exports",
        ["transform-import-to-read-file-sync", {
            "test": "\\.css$",
            "options": "utf8"
        }]
    ]
}
